{
  "name": "smartbill-ts-sdk",
  "version": "0.0.1",
  "author": "florin-szilagyi",
  "description": "SmartBill TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
